{{#with data.validUser}}
<div>
    {{> createAndSearch}}
    {{#if enrolledCourses}}
    <h1 class="text-center">Enrolled Courses</h1>


    <div class="container mt-3">
        <div class="row ">

            <div class="card-deck d-flex justify-content-center">
                {{>courseCard}}

            </div>
        </div>
    </div>
    {{else}}
    <h3 class="text-center">No courses...</h3>
    {{/if}}
</div>
{{/with}}